    "build": "cross-env NODE_OPTIONS=--max-old-space-size=6144 NODE_ENV=production webpack",
    "serve": "serve dist -p 3001",
    "dev": "cross-env NODE_ENV=development PUBLIC_URL=https://localhost:3001 API_URL=http://localhost:4000 webpack serve",
