{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components": [
        "src/shared/components"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@services": [
        "src/shared/services"
      ],
      "@routes": [
        "src/shared/routes"
      ],
      "@store": [
        "src/store"
      ],
      "@features": [
        "src/features"
      ],
      "@features/*": [
        "src/features/*"
      ],
      "@config": [
        "src/shared/config"
      ],
    }
  }
}